{
  "name": "dcy-crypto-backend",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node -r dotenv-flow/config src/app.js -- --dotenv-flow-path=envs",
    "dev": "cross-env NODE_ENV=development nodemon src/app.js",
    "staging": "cross-env NODE_ENV=staging nodemon src/app.js",
    "prod": "cross-env NODE_ENV=production npm start",
    "deploy: staging": "NODE_ENV=staging gcloud app deploy",
    "deploy: prod": "NODE_ENV=production gcloud app deploy"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
